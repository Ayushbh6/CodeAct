{
  "name": "react-codeact-bot",
  "version": "1.0.0",
  "description": "React CodeAct Learning Bot - Next.js Chat Interface with OpenRouter API",
  "main": "server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "docker:build": "docker-compose -f docker-compose.executor.yml build",
    "docker:up": "docker-compose -f docker-compose.executor.yml up -d",
    "docker:down": "docker-compose -f docker-compose.executor.yml down",
    "docker:logs": "docker-compose -f docker-compose.executor.yml logs -f",
    "executor:build": "cd react-executor && docker build -t react-executor .",
    "executor:run": "docker run -d -p 4000:4000 --name react-executor-container react-executor",
    "executor:stop": "docker stop react-executor-container && docker rm react-executor-container",
    "test:executor": "curl http://localhost:4000/health"
  },
  "dependencies": {
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@radix-ui/react-button": "^1.1.1",
    "@radix-ui/react-card": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "framer-motion": "^11.13.5",
    "lucide-react": "^0.462.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "class-variance-authority": "^0.7.1"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/react-codeact-bot.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "typescript",
    "ai",
    "education",
    "codeact",
    "openrouter",
    "qwen",
    "docker",
    "tailwindcss"
  ],
  "author": "Ayush Bhattacharya",
  "license": "MIT"
}
